{
    "name": "contaoblackforest/contao-system-notification",
    "description": "Contao system notification for Contao OpenSource CMS",
    "keywords": [
        "contao",
        "backend",
        "frontend",
        "system",
        "notification",
        "module"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "homepage": "https://github.com/ContaoBlackforest",
            "role": "Developer"
        },
        {
            "name": "Dominik Tomasi",
            "email": "dominik.tomasi@gmail.com",
            "homepage": "https://github.com/ContaoBlackforest",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "baumann.sv@gmail.com",
        "issues": "https://github.com/ContaoBlackforest/contao-system-notification/issues",
        "wiki": "https://github.com/ContaoBlackforest/contao-system-notification/wiki",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/ContaoBlackforest/contao-system-notification"
    },
    "require": {
        "php": ">=5.4",
        "contao/core": ">=3.5,<3.6",
        "contao-community-alliance/composer-plugin": "~2.0",
        "contao-community-alliance/event-dispatcher": "1.3",
        "contao-community-alliance/dependency-container": "~1.8"
    },
    "require-dev": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ContaoBlackForest\\SystemNotification\\": ""
        },
        "classmap": [
            "Resources/contao"
        ],
        "exclude-from-classmap": [
            "Resources/contao/config",
            "Resources/contao/dca",
            "Resources/contao/languages",
            "Resources/contao/templates"
        ]
    },
    "extra": {
        "contao": {
            "sources": {
                "Resources/contao": "system/modules/system-notification",
                "Resources/contao-assets": "assets/system-notification"
            }
        },
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
